<?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.vaayu.database.vo.PaymentVO" table="Payment">
		<id name="paymentID">
			<generator class="increment"></generator>
		</id>

		<property name="billID" />
		<property name="amount" />
		<property name="amountType" />
		<property name="chequeNo" />
		<property name="chequeIssueDate" />
		<property name="receivedDate" />
		<property name="receiver" />
		<property name="createdBy" />
		<property name="createdTimestamp" />
	</class>

</hibernate-mapping>